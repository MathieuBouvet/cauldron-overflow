{% extends 'base.html.twig' %}
{% block title %}
  {{ parent() }}
  Question {{ question }}
{% endblock %}
{% block body %}
  <h1>
    {{ question }}
  </h1>
  <div>
    Eventually, we'll print the full question here!
  </div>
  <div>
    {{ answers|length }}
  </div>
  <ul>
    {% for answer in answers %}
      <li>{{ answer }}</li>
    {% endfor %}
  </ul>
{% endblock %}
